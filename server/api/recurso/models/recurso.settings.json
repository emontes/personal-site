{
  "connection": "default",
  "collectionName": "recursos",
  "info": {
    "name": "recurso"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "used": {
      "type": "date"
    },
    "image": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "url": {
      "type": "string"
    },
    "recurso_tipo": {
      "model": "recurso-tipo"
    }
  }
}
